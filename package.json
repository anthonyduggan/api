    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@bugsnag/js": "^6.4.3",
    "@bugsnag/plugin-koa": "^6.4.1",
    "@koa/cors": "^3.0.0",
    "argon2": "^0.25.1",
    "aws-sdk": "^2.576.0",
    "aws-xray-sdk-core": "^2.5.0",
    "debug": "^4.1.1",
